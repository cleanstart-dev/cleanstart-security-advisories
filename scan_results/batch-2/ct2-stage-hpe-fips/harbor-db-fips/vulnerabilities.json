        {
          "created": "2025-10-15T11:16:02Z",
          "created_by": "RUN /bin/sh -c apk upgrade \u0026\u0026     rm -f /etc/ssl/openssl.cnf \u0026\u0026     apk add --force-overwrite openssl-provider-fips-hpe=3.0.16-r2 \u0026\u0026     rm -rf /usr/libexec/openssl-fips-provider 2\u003e/dev/null || true \u0026\u0026     OPENSSL_CONF=dne openssl fipsinstall -module /usr/lib/ossl-modules/fips.so -out /etc/ssl/fipsmodule.cnf \u0026\u0026     rm -rf /pkgdir/ \u0026\u0026     rm -rf /usr/include/openssl/ \u0026\u0026     /usr/bin/fips_on \u0026\u0026     ln -sf /usr/share/harbor/db/scripts/docker-entrypoint.sh /usr/bin/docker-entrypoint.sh \u0026\u0026     chmod 755 /usr/bin/docker-entrypoint.sh \u0026\u0026     cp /usr/share/harbor/db/scripts/initdb.sh /initdb.sh \u0026\u0026     cp /usr/share/harbor/db/scripts/upgrade.sh /upgrade.sh \u0026\u0026     mkdir -p /var/lib/postgresql/data /run/postgresql /usr/libexec/postgresql14 /usr/libexec/postgresql15 \u0026\u0026     chown -R 999:clngroup /var/lib/postgresql /run/postgresql /usr/libexec/postgresql14 /usr/libexec/postgresql15 \u0026\u0026     chmod 755 /initdb.sh /upgrade.sh \u0026\u0026     chown -R root:root /usr/lib /usr/bin \u0026\u0026     chmod -R 755 /usr/lib /usr/bin \u0026\u0026     find /usr/lib -type f -exec chmod 644 {} \\; \u0026\u0026     find /usr/lib -type d -exec chmod 755 {} \\; \u0026\u0026     cp /etc/passwd /etc/passwd.bak \u0026\u0026     echo \"root:x:0:0:root:/root:/bin/sh\" \u003e /etc/passwd \u0026\u0026     echo \"999:x:999:999:clnstrt:/home/clnstrt:/bin/sh\" \u003e\u003e /etc/passwd \u0026\u0026     echo \"nobody:x:65534:65534:nobody:/:/sbin/nologin\" \u003e\u003e /etc/passwd \u0026\u0026     echo \"root:*:19000:0:72:7:35::\" \u003e /etc/shadow \u0026\u0026     echo \"999:*:19000:0:72:7:35::\" \u003e\u003e /etc/shadow \u0026\u0026     echo \"nobody:*:19000:0:72:7:35::\" \u003e\u003e /etc/shadow \u0026\u0026     chown root:root /etc/shadow \u0026\u0026     chmod 400 /etc/shadow \u0026\u0026     printf \"https://clnpkgs.clnstrt.dev/main\" \